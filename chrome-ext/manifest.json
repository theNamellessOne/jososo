{
  "manifest_version": 3,
  "name": "Hello Extensions",
  "description": "Base Level Extension",
  "version": "1.0",
  "permissions": ["cookies"],
  "host_permissions": [
    "*://*.linkedin.com/*",
    "*://*.jobsenderbot.com/*",
    "*://localhost/*"
  ],
  "action": {
    "default_popup": "index.html",
    "default_icon": "logo.png"
  }
}
